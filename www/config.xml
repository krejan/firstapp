<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0"
	id="com.vphone.firstapp" version="1.0.0">
	<name>firstapp</name>

	<description>
		A sample Apache Cordova application that responds to the
		deviceready
		event.
	</description>

	<author href="http://www.eclipse.org/thym" email="thym-dev@eclipse.org">
		Eclipse.org -
		Thym
	</author>

	<content src="index.html" />

	<access origin="*" />
	<preference name="fullscreen" value="true" />
	<preference name="webviewbounce" value="true" />

	<feature name="App">
		<param name="android-package" value="org.apache.cordova.App" />
	</feature>
	<feature name="Battery">
		<param name="android-package" value="org.apache.cordova.batterystatus.BatteryListener" />
		<param name="ios-package" value="CDVBattery" />
		<param name="wp-package" value="Battery" />
		<param name="id" value="org.apache.cordova.battery-status" />
	</feature>
	<feature name="Camera">
		<param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
		<param name="ios-package" value="CDVCamera" />
		<param name="wp-package" value="Camera" />
		<param name="id" value="org.apache.cordova.camera" />
	</feature>

</widget>
